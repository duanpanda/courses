<?xml version='1.0' encoding='utf-8'?>
<widget id="app.ryan.dirorganizer" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <name>Directory Organizer</name>
    <description>
        A File System Explorer (Directory Organizer).
    </description>
    <author email="rduan@lakeheadu.ca" href="http://cordova.io">
        Ryan Duan
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
